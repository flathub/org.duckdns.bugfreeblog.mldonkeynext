        {
          "type": "archive",
          "url": "https://github.com/carlonluca/mldonkey-next/releases/download/v1.2.1/webapp-v1.2.1.zip",
          "dest": "app/webapp",
          "sha256": "381e67791e31caea60a900aeb4ecbdda6418997381cd9de07feb3e5380868ab3"
        }
      ]
